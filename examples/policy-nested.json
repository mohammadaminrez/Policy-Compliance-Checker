{
  "name": "Nested Account Safeguards",
  "allOf": [
    {
      "field": "profile.security.password.length",
      "operator": ">=",
      "value": 12
    },
    {
      "field": "profile.security.password.last_rotated_days",
      "operator": "<=",
      "value": 30
    },
    {
      "field": "profile.security.mfa.enabled",
      "operator": "==",
      "value": true
    },
    {
      "field": "profile.account.status",
      "operator": "==",
      "value": "active"
    }
  ],
  "anyOf": [
    {
      "field": "profile.account.age_days",
      "operator": ">=",
      "value": 90
    },
    {
      "field": "profile.region",
      "operator": "regex",
      "value": "^us-"
    }
  ]
}
