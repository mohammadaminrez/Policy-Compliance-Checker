{
  "clinical_rules": [
    {
      "id": "AGE001",
      "condition": "patient_age",
      "must_be": "greater than or equal to",
      "threshold": 18
    },
    {
      "id": "TEMP001",
      "condition": "body_temperature",
      "must_be": "less than",
      "threshold": 100.4
    },
    {
      "id": "BP001",
      "condition": "blood_pressure_systolic",
      "must_be": "at most",
      "threshold": 140
    },
    {
      "id": "MED001",
      "condition": "medication",
      "must_be": "one of",
      "threshold": ["aspirin", "ibuprofen", "acetaminophen"]
    },
    {
      "id": "STATUS001",
      "condition": "patient_status",
      "must_be": "equals",
      "threshold": "stable"
    }
  ]
}
